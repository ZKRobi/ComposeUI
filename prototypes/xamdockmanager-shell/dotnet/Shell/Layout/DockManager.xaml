<UserControl x:Class="MorganStanley.ComposeUI.Shell.Layout.DockManager"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MorganStanley.ComposeUI.Shell.Layout"
             xmlns:igDock="http://infragistics.com/DockManager"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <igDock:XamDockManager Name="Root" InitializePaneContent="Root_InitializePaneContent">
            <igDock:XamDockManager.Panes>
                <igDock:SplitPane Name="RootPane"></igDock:SplitPane>
            </igDock:XamDockManager.Panes>
            <igDock:DocumentContentHost Name="Content"></igDock:DocumentContentHost>
        </igDock:XamDockManager>
    </Grid>
</UserControl>
